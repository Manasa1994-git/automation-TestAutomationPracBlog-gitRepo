
<suite name="Regression Suite">
  <test name="Regression Tests with Exclusion">
    <groups>
      <run>
    <include name="regression"/><!-- it has 2 regression methods -->
    <exclude name="sanity"/> <!-- This will skip sanity tests -->
      </run>
    </groups>
    <classes>
      <class name="testcases.TestNGexamples.RegressionTestExample"/>
    </classes>
  </test>
</suite>



